#include "pwm.h"
#include "car.h"
#include <stdio.h>
#include <inttypes.h>

int main()
{
	//receive from XBEE UART
	uint16_t *rx_buff;
	rx_buff[0] = 0x00ff;
	rx_buff[1] = 0xff00;
	rx_buff[2] = 0x0000;

	initMotor();
	while( 1 )
	{
		initMotor();
		//setMotorAPB(rx_buff);
	}
}
